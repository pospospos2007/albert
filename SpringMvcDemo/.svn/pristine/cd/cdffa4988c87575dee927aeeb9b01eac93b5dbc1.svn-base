/*** 主页 ***/
.index{height:46px;background:#f74172;margin-bottom:1px}
.index h1 img{margin:8px 10px 0;cursor:pointer;width:30px}
.index h1{font-size:20px;text-align:center;color:white;line-height:46px}
.index_b {position:relative;width:100%;overflow:auto;text-align:center;color: rgba(255,255,255,.6);text-shadow: 0 0 1px rgba(0,0,0,.05), 0 1px 2px rgba(0,0,0,.3);box-shadow: 0 1px 2px rgba(0,0,0,.25)}
.index_b ul {list-style: none;width:300%}
.index_b ul li {float:left;box-shadow: inset 0 -3px 6px rgba(0,0,0,.1)}		
.index_b .dot {
	-webkit-filter: drop-shadow(0 1px 2px rgba(0,0,0,.3));
	-moz-filter: drop-shadow(0 1px 2px rgba(0,0,0,.3));
	-ms-filter: drop-shadow(0 1px 2px rgba(0,0,0,.3));
	-o-filter: drop-shadow(0 1px 2px rgba(0,0,0,.3));
	filter: drop-shadow(0 1px 2px rgba(0,0,0,.3))
}	
.index_b .dots {position: absolute;left: 0;right: 0;bottom:8px;line-height:18px}
.index_b .dots li {
	display: inline-block;
	width: 10px;
	*zoom:1;
	*display:inline;
	height: 10px;
	margin: 0 5px;
	text-indent: -999em;	
	border: 2px solid #fff;
	border-radius: 6px;	
	cursor: pointer;
	opacity: .4;
	
	-webkit-transition: background .5s, opacity .5s;
	-moz-transition: background .5s, opacity .5s;
	transition: background .5s, opacity .5s
}
.index_b .dots li.active {background: #fff;opacity: 1}
			
